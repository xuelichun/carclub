package com.zeepn.bean;

import org.apache.ibatis.type.Alias;
import org.springframework.stereotype.Component;

@Component
@Alias("UserMessage")
public class UserMessage {
	private int mu_id;
	private int m_show;
	private String m_con;
	private String  m_time;
	private String  mt_title;
	public UserMessage() {
	}
	public UserMessage(int mu_id, int m_show, String m_con, String m_time,
			String mt_title) {
		this.mu_id = mu_id;
		this.m_show = m_show;
		this.m_con = m_con;
		this.m_time = m_time;
		this.mt_title = mt_title;
	}
	public int getMu_id() {
		return mu_id;
	}
	public void setMu_id(int mu_id) {
		this.mu_id = mu_id;
	}
	public int getM_show() {
		return m_show;
	}
	public void setM_show(int m_show) {
		this.m_show = m_show;
	}
	public String getM_con() {
		return m_con;
	}
	public void setM_con(String m_con) {
		this.m_con = m_con;
	}
	public String getM_time() {
		return m_time;
	}
	public void setM_time(String m_time) {
		this.m_time = m_time;
	}
	public String getMt_title() {
		return mt_title;
	}
	public void setMt_title(String mt_title) {
		this.mt_title = mt_title;
	}
	@Override
	public String toString() {
		return "UserMessage [mu_id=" + mu_id + ", m_show=" + m_show
				+ ", m_con=" + m_con + ", m_time=" + m_time + ", mt_title="
				+ mt_title + "]";
	}
	
	
	
}
