package com.zeepn.bean;

import org.apache.ibatis.type.Alias;
import org.springframework.stereotype.Component;
@Component
@Alias("FusMsg")
public class FusMsg {
	private int fm_id;
	private String fm_msg;
	private String fm_date;
	private int fm_show;
	public FusMsg() {
		super();
		// TODO Auto-generated constructor stub
	}
	public FusMsg(int fm_id, String fm_msg, String fm_date, int fm_show) {
		super();
		this.fm_id = fm_id;
		this.fm_msg = fm_msg;
		this.fm_date = fm_date;
		this.fm_show = fm_show;
	}
	public FusMsg(String fm_msg, String fm_date, int fm_show) {
		super();
		this.fm_msg = fm_msg;
		this.fm_date = fm_date;
		this.fm_show = fm_show;
	}
	@Override
	public String toString() {
		return "FusMsg [fm_id=" + fm_id + ", fm_msg=" + fm_msg + ", fm_date="
				+ fm_date + ", fm_show=" + fm_show + "]";
	}
	public int getFm_id() {
		return fm_id;
	}
	public void setFm_id(int fm_id) {
		this.fm_id = fm_id;
	}
	public String getFm_msg() {
		return fm_msg;
	}
	public void setFm_msg(String fm_msg) {
		this.fm_msg = fm_msg;
	}
	public String getFm_date() {
		return fm_date;
	}
	public void setFm_date(String fm_date) {
		this.fm_date = fm_date;
	}
	public int getFm_show() {
		return fm_show;
	}
	public void setFm_show(int fm_show) {
		this.fm_show = fm_show;
	}


}
