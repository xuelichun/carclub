package com.zeepn.bean;

import org.apache.ibatis.type.Alias;
import org.springframework.stereotype.Component;

@Component
@Alias("Message")
public class Message {
	private int m_id;
	private int ga_id;
	private int mu_id;
	private int mt_id;
	private int m_show;
	private String m_con;
	private String m_time;
	public Message() {
		
	}
	public Message(int m_id, int ga_id, int mu_id, int mt_id, int m_show,
			String m_con, String m_time) {
		super();
		this.m_id = m_id;
		this.ga_id = ga_id;
		this.mu_id = mu_id;
		this.mt_id = mt_id;
		this.m_show = m_show;
		this.m_con = m_con;
		this.m_time = m_time;
	}
	public Message(int ga_id, int mu_id, int mt_id, int m_show, String m_con,
			String m_time) {
		super();
		this.ga_id = ga_id;
		this.mu_id = mu_id;
		this.mt_id = mt_id;
		this.m_show = m_show;
		this.m_con = m_con;
		this.m_time = m_time;
	}
	public Message(int ga_id, int mu_id, int mt_id, String m_con) {
		super();
		this.ga_id = ga_id;
		this.mu_id = mu_id;
		this.mt_id = mt_id;
		this.m_con = m_con;
	}
	@Override
	public String toString() {
		return "Message [m_id=" + m_id + ", ga_id=" + ga_id + ", mu_id="
				+ mu_id + ", mt_id=" + mt_id + ", m_show=" + m_show
				+ ", m_con=" + m_con + ", m_time=" + m_time + "]";
	}
	public int getM_id() {
		return m_id;
	}
	public void setM_id(int m_id) {
		this.m_id = m_id;
	}
	public int getGa_id() {
		return ga_id;
	}
	public void setGa_id(int ga_id) {
		this.ga_id = ga_id;
	}
	public int getMu_id() {
		return mu_id;
	}
	public void setMu_id(int mu_id) {
		this.mu_id = mu_id;
	}
	public int getMt_id() {
		return mt_id;
	}
	public void setMt_id(int mt_id) {
		this.mt_id = mt_id;
	}
	public int getM_show() {
		return m_show;
	}
	public void setM_show(int m_show) {
		this.m_show = m_show;
	}
	public String getM_con() {
		return m_con;
	}
	public void setM_con(String m_con) {
		this.m_con = m_con;
	}
	public String getM_time() {
		return m_time;
	}
	public void setM_time(String m_time) {
		this.m_time = m_time;
	}
	
}