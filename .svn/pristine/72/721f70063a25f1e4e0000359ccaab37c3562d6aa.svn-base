package com.zeepn.utils;

public class CompObUtils {
	private boolean flag=false;
	/**
	 * 比较两个整数
	 * @param num1 第一个整数
	 * @param num2 第二个整数
	 * @return 如果第一个整数>第二个整数，返回true；否则返回false
	 */
	public boolean CompareInt1(int num1,int num2){
		if(num1>num2){
			flag=true;
		}
		return flag;
	}
	/**
	 * 比较两个整数
	 * @param num1 第一个整数
	 * @param num2 第二个整数
	 * @return 如果第一个整数>=第二个整数，返回true；否则返回false
	 */
	public boolean CompareInt2(int num1,int num2){
		if(num1>=num2){
			flag=true;
		}
		return flag;
	}
	/**
	 * 比较两个字符串
	 * @param str1 第一个字符串
	 * @param str2 第二个字符串
	 * @return 如果两个字符串两个相等，返回true；否则返回false
	 */
	public boolean compareString(String str1,String str2){
		if(str1.equals(str2)){
			flag=true;
		}
		return flag;
	}
}
