var club_id=0;
var u_id=0;
$(function(){
	yanzhenPre();
	alert(club_id);
	$("#club_id").val(club_id);
	alert($("#club_id").val());
});

$(document).ready(function() { 
    // bind 'myForm' and provide a simple callback function 
    $('#modifyClubForm').ajaxForm(function(data) { 
    	alert(data);
    	//location.href="uploadClubAlbum.html";
    });    
}); 

function yanzhenPre(){
	$.ajax({
		url:'../clubAdminai.do',
		type:'GET',
		dataType:'json',
		async:false,
		success:function(data){
			if(data[0]==2){
				u_id=data[1];
			}else if(data[0]==3){
				u_id=data[1];
				$("#clubFeeLi").hide();
			}else{
				location.href="index.html";
			}
		}	
	});
	$.ajax({
		url:'../clubAdminah.do',
		type:'GET',
		dataType:'json',
		async:false,
		data:"u_id="+u_id,
		success:function(data){
			club_id=data;
		}
	});
}
