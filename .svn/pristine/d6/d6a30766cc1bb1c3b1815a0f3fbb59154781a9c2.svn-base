package com.zeepn.bean;

import org.apache.ibatis.type.Alias;
import org.springframework.stereotype.Component;
@Component
@Alias("Adv")
public class Adv {
	private int av_id;
	private String av_an;
	private String av_a;
	private String av_price;
	private String av_content;
	private int av_click;
	public Adv() {
	}
	public Adv(String av_an, String av_a, String av_price, String av_content,
			int av_click) {
		this.av_an = av_an;
		this.av_a = av_a;
		this.av_price = av_price;
		this.av_content = av_content;
		this.av_click = av_click;
	}
	public Adv(int av_id, String av_an, String av_a, String av_price,
			String av_content, int av_click) {
		this.av_id = av_id;
		this.av_an = av_an;
		this.av_a = av_a;
		this.av_price = av_price;
		this.av_content = av_content;
		this.av_click = av_click;
	}
	public int getAv_id() {
		return av_id;
	}
	public void setAv_id(int av_id) {
		this.av_id = av_id;
	}
	public String getAv_an() {
		return av_an;
	}
	public void setAv_an(String av_an) {
		this.av_an = av_an;
	}
	public String getAv_a() {
		return av_a;
	}
	public void setAv_a(String av_a) {
		this.av_a = av_a;
	}
	public String getAv_price() {
		return av_price;
	}
	public void setAv_price(String av_price) {
		this.av_price = av_price;
	}
	public String getAv_content() {
		return av_content;
	}
	public void setAv_content(String av_content) {
		this.av_content = av_content;
	}
	public int getAv_click() {
		return av_click;
	}
	public void setAv_click(int av_click) {
		this.av_click = av_click;
	}
	@Override
	public String toString() {
		return "Adv [av_id=" + av_id + ", av_an=" + av_an + ", av_a=" + av_a
				+ ", av_price=" + av_price + ", av_content=" + av_content
				+ ", av_click=" + av_click + "]";
	}
}
